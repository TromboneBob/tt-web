    "JavaScript": {
      "language_servers": [
        "typescript-language-server",
        "!deno",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    },
    "TypeScript": {
      "language_servers": [
        "typescript-language-server",
        "!deno",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    },
    "TSX": {
      "language_servers": [
        "typescript-language-server",
        "!deno",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    },
    "Vue.js": {
      "language_servers": ["typescript-language-server", "!deno"],
      "formatter": "language_server"
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
